#include <stdio.h>

int main() {
	//code
	int n;
	scanf("%d",&n);
	int i;
char arr[n][1000];
char string[n][1000];
for(i=0;i<n;i++)
scanf("%s",&string[i][0]);

for(i=0;i<n;i++)
printf("%s\n",string[i]);
	
	return 0;
}
